DISCORD_TOKEN=

# App Mongo connection (use app user, not root)
MONGO_URI=mongodb://feedbot:feed_password@localhost:27017/feed_database?authSource=feed_database
MONGO_DB=feed_database
MONGO_MESSAGE_MAPPING_COLLECTION=message_mappings
MONGO_GUILD_ROUTES_COLLECTION=guild_routes
MONGO_GUILD_PERMISSIONS_COLLECTION=guild_permissions

# Logging
LOG_FILE=logs/feed_bot.log

# Mongo container init
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=change_me
MONGO_INITDB_DATABASE=feed_database
MONGO_APP_USERNAME=feedbot
MONGO_APP_PASSWORD=feed_password
MONGO_APP_DB=feed_database
